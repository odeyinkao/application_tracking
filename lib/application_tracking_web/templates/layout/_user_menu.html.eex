<ul>
<%= if @current_user do %>
  <li><%= @current_user.email %></li>
  <li><%= link "Log out", to: Routes.user_session_path(@conn, :delete), method: :delete %></li>
  <li><%= link "Member Register", to: Routes.user_registration_path(@conn, :new) %></li>
<% else %>
  <li><%= link "Member Login", to: Routes.user_session_path(@conn, :new) %></li>
<% end %>
</ul>
